{
  "expo": {
    "name": "Travel Bucket List",
    "slug": "travel-bucket",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "backgroundColor": "#F5F5F1",
    "primaryColor": "#F5F5F1",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F5F1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.josebravo.travelapp"
    },
    "android": {
      "package": "com.josebravo.travelapp"
    },
    "androidStatusBar": {
      "backgroundColor": "#F5F5F1",
      "barStyle": "light-content"
    }
  }
}
